    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "fs-extra": "^8.1.0",
    "json-server": "^0.15.1",
    "path": "^0.12.7",
    "prettier": "^1.16.4",
